- var i = 1
- var j = 1
- var limit = 10

.section.section-project

	h2.section__title Project Carousel
	small.section__subtitle 
		| A simple Synchronised carousel by 
		a(href="https://codepen.io/ariona") @ariona_rian
		br
		| using Slick Carousel
	
	.project-carousel
	
		.project-strip
		
			while i++ < limit
				.project
					img(src=".\images\anis.jpg" + i * 13 + "&blur", alt="")


		.project-screen
		
			.project-detail
			
				while j++ < limit
					.project
						img(src="\images\imagesblur\clove.jpg" + j * 13 , alt="")
				
			.screen-frame